{
    "name": "mateusjatenee/laravel-notifications-plivo",
    "description": "Plivo SMS notifications driver for Laravel",
    "keywords": [
        "laravel",
        "notifications",
        "plivo"
    ],
    "homepage": "https://github.com/mateusjatenee/laravel-notifications-plivo",
    "license": "MIT",
    "authors": [
        {
            "name": "Sid K",
            "email": "sid@koomai.net",
            "homepage": "http://koomai.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/notifications": "~5.5 || ~6.0",
        "illuminate/support": "~5.5 || ~6.0",
        "illuminate/events": "~5.5 || ~6.0",
        "illuminate/queue": "~5.5 || ~6.0",
        "plivo/plivo-php": "^1.1"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "Mateusjatenee\\Plivo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mateusjatenee\\Plivo\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
